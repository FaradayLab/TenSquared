<?xml version='1.0' encoding='utf-8'?>
<widget id="com.faradaycreative.tensquared" version="0.8.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>10 Squared</name>
    <description>
        A 10x10 Game
    </description>
<author email="alexanderwilkins1@gmail.com" href="http://phonegap.com">
        Faraday Creative
    </author>
    <icon src="icon.png"/>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.3" />

    <!-- <plugin name="phonegap-admob" source="npm" /> -->
    <!-- <gap:plugin name="phonegap-admob" source="npm"/> -->
    <access origin="*" />
    <!-- <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" /> -->
    <preference name="cdvMinSdkVersion" value="10"/>
    <preference name="cdvCompileSdkVersion" value="20"/>
    <preference name="prerendered-icon" value="true"/>
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="DisallowOverscroll" value="true" />
    <!-- <preference name="SplashScreen" value="screen" /> -->
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="FadeSplashScreen" value="false"/>
    <preference name="ShowSplashScreenSpinner" value="false"/>
    <preference name="SetFullscreen" value="true" />
    <preference name="DefaultVolumeStream" value="media" />
    <preference name="Orientation" value="portrait" />


    <platform name="android">
        <allow-intent href="market:*" />
        
        <splash src="splash.png" />
        <splash src="res/screen/android/splash.png" />
        <splash src="res/screen/android/splash-port-hdpi.png" density="port-hdpi"/>
        <splash src="res/screen/android/splash-port-ldpi.png" density="port-ldpi"/>
        <splash src="res/screen/android/splash-port-mdpi.png" density="port-mdpi"/>
        <splash src="res/screen/android/splash-port-xhdpi.png" density="port-xhdpi"/>
        <splash src="res/screen/android/splash-port-xxhdpi.png" density="port-xxhdpi"/>
        
    </platform>

    <!-- iOS only preferences -->
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
